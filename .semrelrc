{
  "plugins": {
    "commit-analyzer": {
      "name": "default@^1.0.0",
      "options": {
        "minor_release_rules": "feat,chore(deps)"
      }
    },
    "ci-condition": {
      "name": "default"
    },
    "changelog-generator": {
      "name": "default",
      "options": {
        "emojis": "true"
      }
    },
    "provider": {
      "name": "github",
      "options": {
        "slug": "bbeesley/fn-push"
      }
    }
  }
}
